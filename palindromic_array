class palindrome{
public static void isPalindromeArr(int[] arr,int n){
int flag=0;
for(int i=0;i<n/2;i++){
if(arr[i]!=arr[n-i-1]){
flag=1;
System.out.println("not a palindromic array");
break;
}
}
if(flag==0){
System.out.println("palindromic array");
}

public static void main(String[] args){
Scanner sc=new Scanner(System.in);
System.out.println("enter the size of an array");
int n=sc.nextInt;
int []arr=new int[n];
for(int i=0;i<n;i++){
arr[i]=sc.nextInt();
}
isPalindromeArr(arr,n);
}
}
